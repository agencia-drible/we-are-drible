{% extends 'layout' %}
{% block main %}
<main class="bd-masthead" id="content" role="main">
  <div class="container">
    <div class="row">
      <div class="col-lg-10 col-sm-12 col-md-12 mx-auto">
        {% for block in entry.matriz %}
        <h1 id="{{ entry.slug }}">{{ entry.title }}</h1>
        <br>
        {{ block.body }}
        {% endfor %}
      </div>
    </div>
  </div>
</main>
{% endblock %}
